{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "quotes": [2, "double", { "avoidEscape": true }],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "import/extensions": ["error", "ignorePackages"]
  }
}
